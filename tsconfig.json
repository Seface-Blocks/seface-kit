{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "typings",
    "strictNullChecks": false,
    "noImplicitAny": false,
    "allowJs": true,
    "resolveJsonModule": true,
    "target": "es2017",
    "module": "commonjs",
    "outDir": "./build",
    "isolatedModules": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@command/*": ["./src/command/*"],
      "@event/*": ["./src/event/*"],
      "@utils/*": ["./src/utils/*"],
      "@services/*": ["./src/services/*"],
      "@interfaces/*": ["./src/interfaces/*"],
    }
  },

  "include": ["./src/**/*.ts"],
  "exclude": [ ]
}
